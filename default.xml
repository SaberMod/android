<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="korg"
           fetch="git://android.git.linaro.org/"
           review="review.android.git.linaro.org" />

  <remote  name="linaro-android"
           fetch="git://android.git.linaro.org/"
           review="review.android.git.linaro.org" />

  <remote  name="linaro-other"
           fetch="git://git.linaro.org/" />

  <default revision="linaro-master"
           remote="linaro-android" />

  <project path="benchmark" name="toolchain/benchmark" revision="master" />
  <project path="binutils" name="toolchain/binutils" />
  <project path="build" name="toolchain/build" />
  <project path="gcc-patches" name="toolchain/gcc-patches" revision="master" />
  <project path="gdb" name="toolchain/gdb" />
  <project path="gmp" name="toolchain/gmp" />
  <project path="mpc" name="toolchain/mpc" />
  <project path="mpfr" name="toolchain/mpfr" />
  <project path="ppl" name="toolchain/ppl" revision="master" />
  <project path="cloog" name="toolchain/cloog" revision="master" />

  <!-- Bionic and the minimal parts of the Android build system needed to build it - needed for libgomp and friends -->
  <project path="android/build" name="platform/build" revision="linaro_android_4.1.1">
    <copyfile dest="android/Makefile" src="core/root.mk"/>
  </project>
  <project name="device/common" path="android/device/common" revision="master"/>
  <project name="device/linaro/common" path="android/device/linaro/common" revision="linaro-ics"/>
  <project name="device/linaro/pandaboard" path="android/device/linaro/pandaboard" revision="linaro-jb"/>
  <project name="platform/hardware/ti/omap4xxx" path="android/hardware/ti/omap4xxx" revision="linaro_android_4.1.1"/>
  <project name="platform/frameworks/base" path="android/frameworks/base" revision="linaro_android_4.1.1"/>
  <project name="platform/frameworks/native" path="android/frameworks/native" revision="linaro_android_4.1.1"/>
  <project name="platform/external/elfutils" path="android/external/elfutils" revision="linaro_android_4.1.1"/>
  <project name="platform/external/svox" path="android/external/svox" revision="refs/tags/android-4.1.1_r4"/>
  <project name="platform/system/core" path="android/system/core" revision="linaro_android_4.1.1" />
  <project path="android/bionic" name="platform/bionic" revision="linaro_android_4.1.1" />
</manifest>
